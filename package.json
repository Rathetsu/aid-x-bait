{
	"name": "aid-x-bait",
	"main": "expo-router/entry",
	"version": "1.0.0",
	"scripts": {
		"start": "expo start",
		"--- EAS Prebuild": "",
		"prebuild:dev": "NODE_ENV=development expo prebuild --clean; node fixKotlinClasspath.js;",
		"prebuild:preview": "NODE_ENV=test expo prebuild --clean; node fixKotlinClasspath.js;",
		"prebuild:local": "NODE_ENV=local expo prebuild --clean; node fixKotlinClasspath.js;",
		"--- EAS Start local build": "",
		"start:ios:dev": "NODE_ENV=development expo run:ios --device",
		"start:ios:preview": "NODE_ENV=test expo run:ios --configuration Release --device",
		"start:ios:prod": "NODE_ENV=production expo run:ios --configuration Release --device",
		"start:ios:local": "NODE_ENV=local expo run:ios --device",
		"start:android:dev": "NODE_ENV=development expo run:android --device",
		"start:android:preview": "NODE_ENV=test expo run:android --variant release --device",
		"start:android:prod": "NODE_ENV=production expo run:android --variant release --device",
		"start:android:local": "NODE_ENV=local expo run:android --device",
		"--- EAS Build scripts": "",
		"build:ios:prod": "eas build -p ios -e production --clear-cache",
		"build:ios:preview": "eas build -p ios -e preview --clear-cache",
		"build:ios:dev": "eas build -p ios -e development --clear-cache",
		"build:android:prod": "eas build -p android -e production --clear-cache",
		"build:android:preview": "eas build -p android -e preview --clear-cache",
		"build:android:dev": "eas build -p android -e development --clear-cache",
		"--- EAS Submit scripts": "",
		"submit:ios": "NODE_ENV=production eas submit -p ios",
		"submit:ios:local": "NODE_ENV=local eas submit -p ios",
		"submit:android": "NODE_ENV=production eas submit -p android",
		"--- EAS Update rollback scripts": "",
		"update:rollback:prod": "NODE_ENV=production eas update:rollback",
		"update:rollback:preview": "NODE_ENV=test eas update:rollback",
		"update:rollback:dev": "NODE_ENV=development eas update:rollback",
		"--- EAS Update deploy scripts": "",
		"update:deploy:preview": "NODE_ENV=test eas update --branch preview --clear-cache && NODE_ENV=test npx sentry-expo-upload-sourcemaps dist",
		"update:deploy:prod": "NODE_ENV=production eas update --branch production --clear-cache && NODE_ENV=production npx sentry-expo-upload-sourcemaps dist",
		"update:deploy:dev": "NODE_ENV=development eas update --branch development --clear-cache && NODE_ENV=development npx sentry-expo-upload-sourcemaps dist",
		"--- Reset watchman process": "",
		"reset-watchman": "watchman watch-del '/Users/dobo/git-repos/aidxbait-app' ; watchman watch-project '/Users/dobo/git-repos/aidxbait-app'",
		"--- MISC": "",
		"android": "expo run:android",
		"ios": "expo run:ios"
	},
	"jest": {
		"preset": "jest-expo"
	},
	"dependencies": {
		"@clerk/clerk-expo": "^2.3.11",
		"@config-plugins/react-native-webrtc": "^10.0.0",
